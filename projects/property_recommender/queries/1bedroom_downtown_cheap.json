{
  "_source": [
    "listing_number", "status", "street_address", "suburb", "price", "list_date", "days_on_market",
    "total_bedrooms", "total_baths", "total_square_foot", "year_built", "age", "locker", "total_parking", "strat_fee", "dwelling_type", "pets_allowed", "rent_allowed"
  ],
  "query": {
    "function_score": {
      "query": {
        "bool": {
          "must": [
            {"range": {"total_parking": {"gte": "1"}}},
            {"range": {"total_square_foot": {"gte": "600"}}}
          ],
          "must_not": [],
          "should": [
            {"range": {"total_square_foot": {"gte": "800"}}},
            {"range": {"price": {"lte": "300000"}}}
          ]
        }
      },
      "functions": [
        {"script_score" : {"script" : {"source": "Math.log(2 + doc['days_on_market'].value)"}}, "weight": 5},
        {"exp": {"price": {"origin": "0", "scale": "50000", "offset": "350000", "decay" : 0.5}}, "weight": 50},
        {"field_value_factor": {"field": "total_square_foot", "modifier": "sqrt", "factor": 1, "missing":  1}}
      ],
      "boost_mode": "multiply"
    }
  }
}
